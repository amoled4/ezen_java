package day03;

public class 최대공약수 {

	public static void main(String[] args) {
		/* 최대공약수 구하기
		 * 공약수 : 두 수의 공통된 약수
		 * 최대공약수 : 두 수의 공통된 약수 중 가장 큰 값
		 * 
		 * 8 : 1 2 4 8
		 * 12 : 1 2 3 4 6 12
		 * 공약수 : 1 2 4
		 * 최대공약수 : 4
		 */
		
		int num1 = 8;
		int num2 = 12;
		// 두 수의 공약수만 출력
		for(int i=1; i<=num1; i++)
			for(int j=1; j<=num2; j++) {
			if(num1 % i == 0 && num2 % i==0) {
				System.out.println(i);
			}
			}

	}

}
