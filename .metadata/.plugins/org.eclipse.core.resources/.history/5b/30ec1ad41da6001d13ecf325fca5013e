package day05;

public class ArrayEx01 {

	public static void main(String[] args) {
		/* 5개 값을 가지는 배열을 생성 후
		 * 1~50사이의 랜덤값을 저장 후 출력
		 */
		
		int arr[] = new int[5];
		
		for(int i=0; i<arr.length; i++) {
			int num = (int)(Math.random()*50)+1;
			arr[i] = num;
			System.out.print(arr[i]+" ");
		}

	}

}
