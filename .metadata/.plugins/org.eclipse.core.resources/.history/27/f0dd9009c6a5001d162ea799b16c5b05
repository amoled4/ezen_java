package day05;

public class ArrayEx01 {

	public static void main(String[] args) {
		/* 5개 값을 가지는 배열을 생성 후
		 * 1~50사이의 랜덤값을 저장 후 출력
		 */
		
		
		// 1. 배열을 정의
		// 2. for 배열의 index 탐색
		// 3. 랜덤값 배열에 저장
		// 4. 랜덤값 출력
		
		int num = 5;
		int arr[] = new int[num];
		
		for(int i=0; i<arr.length; i++) {
			arr[i] = (int)(Math.random()*50) +1;
			System.out.printf("arr[%d] = %d\n", i, arr[i]);
		}
		
		
		
		
		

	}

}
