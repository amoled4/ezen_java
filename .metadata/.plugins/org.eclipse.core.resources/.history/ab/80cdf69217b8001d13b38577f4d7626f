package 원격수업;

import java.util.Random;
import java.util.TreeSet;

public class 과제5 {

	public static void main(String[] args) {
		// 로또 프로그램 Set으로 변환하여 등수 확인하기.
		
		
		TreeSet<Integer> set = new TreeSet<>();
		
		while(set.size()<6) {
			int b = new Random().nextInt(45)+1;
			set.add(b);		
		}
		
		System.out.println(set);

	}

}
