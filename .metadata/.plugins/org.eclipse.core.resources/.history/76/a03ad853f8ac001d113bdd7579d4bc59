package day12;

public class Exception03 {

	public static void main(String[] args) {
		/* 자주 발생하는 예외 코드들
		 * 
		 */
		
		// ArithmeticException : 0으로 나누었을 때 발생
//		double a = 1/0;
//		System.out.println(a);
		
		int arr[] = new int[5];
		for(int i=0; i<=arr.length; i++) {
			System.out.println(arr[i]);
		}

	}

}
