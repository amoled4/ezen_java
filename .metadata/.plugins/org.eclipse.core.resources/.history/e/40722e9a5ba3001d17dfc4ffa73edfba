package day04;

public class While문 {

	public static void main(String[] args) {
		/* while(조건식) {
		 *  실행문;  // 조건식이 false가 될 수 있는 구문을 포함해야 함.
		 *  증감식;
		 * }
		 * 
		 * 초기화
		 * while(조건식) {
		 *  실행문;
		 *  증감식;
		 * }
		 * 
		 */
		/* 1~10까지 출력
		 * 
		 */
		for(int i=1; i<=10; i++) {
			System.out.print(i+" ");
		}

	}

}
