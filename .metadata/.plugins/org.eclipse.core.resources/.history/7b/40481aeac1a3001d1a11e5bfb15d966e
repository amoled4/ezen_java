package day04;

public class 과제 {

	public static void main(String[] args) {
				/* 주사위를 던져서 총 30칸을 이동하면 완주
				 * 주사위 : 1~6까지의 랜덤수
				 * 주사위 : 3
				 * 3칸 전진 => 27칸 남았습니다.
				 * 주사위 : 6
				 * 6칸 전진 => 21칸 남았습니다.
				 * ...
				 * 도착~!! 총 이동횟수 : 00번
				 */
		
		
		int dice = (int)(Math.random()*6)+1;
		System.out.println("주사위 : "+dice);
				
				
				

			

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
