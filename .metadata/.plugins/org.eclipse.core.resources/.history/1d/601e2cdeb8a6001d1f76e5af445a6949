package day06;

import java.util.Scanner;

public class Method06 {

	public static void main(String[] args) {
		/* 구구단 출력
		 * 구구단 1단을 출력하는 메서드
		 * 7 => 7단
		 */
		
		Scanner scan = new Scanner(System.in);
		System.out.println("숫자를 입력해 주세요.");
		int i = scan.nextInt();
		gugudan(i);

	}
	
	public static void gugudan(int num1) {
			int j;	
			for(j=1; j<=9; j++) {
				System.out.println(j);
			}
		
		
	}
}
